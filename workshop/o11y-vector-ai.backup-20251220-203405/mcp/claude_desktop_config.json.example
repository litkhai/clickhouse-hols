{
  "mcpServers": {
    "clickhouse-otel": {
      "command": "uvx",
      "args": ["mcp-clickhouse"],
      "env": {
        "CLICKHOUSE_HOST": "<your-clickhouse-cloud-host>",
        "CLICKHOUSE_PORT": "8443",
        "CLICKHOUSE_USER": "default",
        "CLICKHOUSE_PASSWORD": "<your-password>",
        "CLICKHOUSE_DATABASE": "o11y",
        "CLICKHOUSE_SECURE": "true"
      }
    }
  }
}
